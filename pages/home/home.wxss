#home {
  position: relative;
  width: 100%;
  height: 100%;
  padding: 100rpx 0;
  box-sizing: border-box;
}
#home .btn-wrap {
  position: fixed;
  bottom: 10%;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  align-items: center;
  flex-direction: column;
}
#home .btn-wrap button {
  color: rgba(255, 255, 255, 0.7);
  font-size: 26rpx;
  padding: 10rpx 40rpx;
  box-sizing: border-box;
  border: 1rpx solid rgba(255, 255, 255, 0.7);
  border-radius: 100rpx;
  width: 300rpx;
}
